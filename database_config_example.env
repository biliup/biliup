# 数据库类型配置
# 可选值: sqlite, mysql
# 默认值: sqlite
# 注意: 一旦选择数据库类型，不支持后续迁移，请谨慎选择
BILIUP_DB_TYPE=mysql

# MySQL数据库配置
# MySQL服务器地址
BILIUP_MYSQL_HOST=localhost

# MySQL端口
BILIUP_MYSQL_PORT=3306

# 数据库名称
BILIUP_MYSQL_DATABASE=biliup

# 数据库用户名
BILIUP_MYSQL_USERNAME=biliup

# 数据库密码
BILIUP_MYSQL_PASSWORD=your_password_here

# 字符集
BILIUP_MYSQL_CHARSET=utf8mb4 